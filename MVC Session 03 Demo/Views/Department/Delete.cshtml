@model Department

@{
    ViewData["Title"] = "Delete Department";
}

<h1>Are You Sure You Want To Delete This Department?</h1>

<form asp-action="Delete">

    <partial name="PartialViews/DetailsDeleteDepartmentPartial" />

    <input type="submit" value="Delete" class="btn btn-danger"/>
    <a asp-action="Index" class="btn btn-info">Cancel</a>

</form>

